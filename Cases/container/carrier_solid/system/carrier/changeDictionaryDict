            kappa           none;
            thermalInertia  true;
            value           $internalField;
        }
        carrier_to_airInside
        {
            type            compressible::turbulentTemperatureRadCoupledMixed;
            Tnbr            T;
            kappaMethod     solidThermo;
            qrNbr           none;
            qr              none;
            kappa           none;
            thermalInertia  true;
            value           $internalField;
        }
        carrier_to_trailer
        {
            type zeroGradient;
