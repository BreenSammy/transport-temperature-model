T
{
    internalField   uniform 285;

    boundaryField
    {
        container
        {
            type zeroGradient;
        }
        box_to_airInside
        {
            type            compressible::turbulentTemperatureRadCoupledMixed;
            Tnbr            T;
            kappaMethod     fluidThermo;
            qrNbr           none;
            qr              none;
            // thicknessLayers (0.002);
            // kappaLayers     (40);
            kappa           none;
            thermalInertia  true;
            value           $internalField;
        }
    }
}

p
{
    internalField   uniform 100000;

    boundaryField
    {
    }
}

