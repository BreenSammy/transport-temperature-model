p_rgh
{
    internalField   uniform 1e5;

    boundaryField
    {
        bottom
        {
            type            fixedFluxPressure;
            value           $internalField;
        }
        air
        {
            type            totalPressure;
            p0              $internalField;
            value           $internalField;
        }
        air_to_shippingBox
        {
            type    fixedFluxPressure;
            value   $internalField;
        }
        front
        {
            type            empty;
        }
        back
        {
            type            empty;
        }
    }
}

p
{
    internalField   uniform 1e5;

    boundaryField
    {
        ".*"
        {
            type            calculated;
            value           $internalField;
        }
        front
        {
            type            empty;
        }
        back
        {
            type            empty;
        }
    }
}

