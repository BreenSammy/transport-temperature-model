FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      changeDictionaryDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

U
{
    internalField   uniform (0 0 0);

    boundaryField
    {
        air
        {
            type            pressureInletOutletVelocity;
            value           $internalField;
        }
        bottom
        {
            type            noSlip;
        }
        air_to_shippingBox
        {
            type            noSlip;
        }
        front
        {
            type            empty;
        }
        back
        {
            type            empty;
        }
    }
}

T
{
    internalField   uniform 300;

    boundaryField
    {
        air
        {
            type            zeroGradient;
        }
        bottom
        {
            type            zeroGradient;
        }
        air_to_shippingBox
        {
            type            compressible::turbulentTemperatureCoupledBaffleMixed;
            Tnbr            T;
            kappaMethod     fluidThermo;
            value           uniform 300;
        }
        front
        {
            type            empty;
        }
        back
        {
            type            empty;
        }
    }
}

