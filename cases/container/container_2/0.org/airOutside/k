FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/air";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 2 -2 0 0 0 0 ];

internalField   uniform 0.1;

boundaryField
{
    air
    {
        type            inletOutlet;
        value           uniform 0.1;
        inletValue      uniform 0.1;
    }
    airOutside_to_ground
    {
        type            kqRWallFunction;
        value           uniform 0.1;
        inletValue      uniform 0.1;
    }
    airOutside_to_carrier
    {
        type            kqRWallFunction;
        value           uniform 0.1;
        inletValue      uniform 0.1;
    }
}


