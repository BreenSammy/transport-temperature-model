FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      changeDictionaryDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

T
{
    internalField   uniform 270;

    boundaryField
    {
        symmetryPlanePallet
        {
			type            symmetry;
        }
        pallet
        {
            type            zeroGradient;
        }
        battery0_to_battery1
        {
            type            compressible::turbulentTemperatureCoupledBaffleMixed;            
            Tnbr            T;
            thicknessLayers (0.01);
            kappaLayers     (0.05);
            kappaMethod     directionalSolidThermo;
            alphaAni        Anialpha;
            // thermalInertia  true;
            value           uniform 270;
        }
        battery0_to_air
        {
            type            compressible::turbulentTemperatureCoupledBaffleMixed;            
            Tnbr            T;
            thicknessLayers (0.01);
            kappaLayers     (0.05);
            kappaMethod     directionalSolidThermo;
            // thermalInertia  true;
            alphaAni        Anialpha;
            // thermalInertia  true;
            value           uniform 270;
        }
    }
}

p
{
    internalField   uniform 100000;

    boundaryField
    {
		symmetryPlanePallet
        {
			type            symmetry;
        }
    }
}

